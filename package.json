{
  "main": "index.js",
  "types": "index.d.ts",
  "version": "0.1.5",
  "engines": {
    "node": ">= 8.0.0"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-node"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "test/case/",
    "ui-test/",
    "test/__frag/",
    "test/__gitcase/"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "quoteProps": "consistent",
    "trailingComma": "none"
  },
  "scripts": {
    "eslint": "eslint ./",
    "eslint:fix": "eslint ./ --fix",
    "prettier": "prettier --write ./*.js ./",
    "test": "jest ./test/jest/",
    "test:cache-limit": "jest ./test/jest/cache-limit.test.js",
    "test:query": "jest ./test/jest/query.test.js",
    "test:render-error": "jest ./test/jest/render-error.test.js",
    "test:cache-expire": "jest ./test/jest/cache-expire.test.js",
    "prepublishOnly": "npm run prettier & npm test"
  },
  "dependencies": {
    "yyl-fs": "^0.3.3",
    "yyl-util": "^2.3.8"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-yyl-node": "^0.1.2",
    "jest": "26.4.2",
    "prettier": "^2.1.2"
  },
  "name": "serve-yyl-ssr",
  "description": "yyl ssr server middleware",
  "repository": "https://github.com/jackness1208/serve-yyl-ssr.git",
  "author": "jackness",
  "license": "MIT"
}
