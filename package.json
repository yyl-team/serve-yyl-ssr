{
  "version": "0.3.2",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/output",
    "/test"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "scripts": {
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rollup --config ./rollup.config.js",
    "d": "rollup --config ./rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "w": "rollup --watch --config ./rollup.config.js",
    "release": "yyr",
    "prepublishOnly": "npm run prettier & npm run build",
    "test": "jest ./test/jest/ --detectOpenHandles",
    "pretest": "npm run d",
    "cbs": "node --inspect ./test/case/base/server.js",
    "cbc": "node ./test/case/base/client.js"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-yyl-react": "^0.1.9",
    "express": "^4.17.1",
    "jest": "26.4.2",
    "prettier": "^2.0.4",
    "rollup": "^1.9.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "supertest": "^6.0.1",
    "typescript": "^3.8.3",
    "yyl-fs": "^0.3.3",
    "yyl-os": "^0.11.2",
    "yyl-release": "0.1.0"
  },
  "name": "serve-yyl-ssr",
  "repository": "https://github.com/jackness1208/serve-yyl-ssr.git",
  "author": "liudaojie <liudaojie@yy.com>",
  "license": "MIT",
  "dependencies": {
    "@types/redis": "^2.8.28",
    "dayjs": "^1.9.6",
    "redis": "^3.0.2",
    "yyl-util": "^2.3.8"
  }
}
